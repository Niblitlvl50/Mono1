{
    "python.linting.pylintEnabled": false,

    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/.vscode": false,
        "**/*.VC.db": true,
        "bin/": true,
        "CMakeFiles": true,
        "cmake_install.cmake": true,
        "CMakeCache.txt": true,
        "Makefile": true
    },
    "files.associations": {
        "ios": "cpp",
        "array": "cpp",
        "string": "cpp",
        "vector": "cpp",
        "algorithm": "cpp",
        "string_view": "cpp",
        "__config": "cpp",
        "__nullptr": "cpp",
        "exception": "cpp",
        "initializer_list": "cpp",
        "new": "cpp",
        "stdexcept": "cpp",
        "typeinfo": "cpp"
    }
}